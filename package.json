    "start": "cm monitor",
    "theme-importer": "cm theme-importer"
  },
  "dependencies": {
    "@coremedia/cm-cli": "^2.0.0",
    "@coremedia/theme-utils": "^3.0.0",
    "@coremedia/brick-page": "^1.0.0",
    "@coremedia/brick-preview": "^1.0.0",
    "@babel/core": "^7.0.0",
    "babel-loader": "^8.2.3",
    "vue": "^2.7.10",
    "vee-validate": "^3.2.2",
    "vue-recaptcha": "^1.3.0",
    "vue-resource": "^1.5.1",
    "vue-router": "^3.3.2",
    "vue-loader": "^15.10.0",
    "vuex": "^3.4.0",
    "vue-datetime": "^1.0.0-beta.14",
    "luxon": "^1.28.0",
    "weekstart": "^1.1.0",
    "bootstrap": "^4.4.1",
    "jquery": "^3.0.0",
    "popper.js": "^1.16.1"
  },
  "devDependencies": {
    "webpack": "^4.0.0",
    "css-loader": "3.3.0",
    "vue-style-loader": "~4.1"
  },
  "main": "src/js/index.js",
  "coremedia": {
    "type": "theme",
    "init": "src/js/core-forms-sample.js"
