{
  "name": "@coremedia/core-forms-sample-theme",
  "version": "1.0.0",
  "private": true,
  "author": "Tallence AG",
  "description": "A sample theme for the Core Forms repository",
  "keywords": [
    "core-forms",
    "forms",
    "theme",
    "coremedia",
